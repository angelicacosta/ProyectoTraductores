with 
	var p, i, n: int
	var a : array[2] of int
	var boleano : bool
begin 
	while (n < 3 /\ true) ->
		i <- 34 * 2 + 5;
		with
			var t, b <- 5 : int
		begin
			for t from 0 to n-1 step 3 ->
				t.1;
			end
		end
	end	
	if boleano ->
		a[1] <- t + 25;
	otherwise ->
		a[2] <- t + b;
		p <- 7;
	end
	p.(3+7);
end